{
  "expo": {
    "android": {
      "jsEngine": "hermes"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "jsEngine": "hermes",
      "supportsTablet": true
    },
    "name": "Template",
    "orientation": "portrait",
    "owner": "binarapps",
    "slug": "expo-typescript-template",
    "splash": {
      "backgroundColor": "#ffffff",
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "userInterfaceStyle": "automatic",
    "version": "2.1.0",
    "plugins": ["expo-font", "expo-localization", "expo-secure-store"]
  }
}
